<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E2929"
    android:elevation="200dp"
    android:visibility="visible">

    <TextView
        android:id="@+id/Titulo"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="27dp"
        android:fontFamily="@font/carme"
        android:gravity="center"
        android:text="@string/bienvenida"
        android:textColor="#FFFFFF"
        android:textSize="35sp" />

    <Button
        android:id="@+id/botonCerrarSesion"
        android:layout_width="204dp"
        android:layout_height="37dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="111dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="33dp"
        android:layout_toStartOf="@+id/fab"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="@font/carme"
        android:text="Cerrar Sesion"
        android:textSize="10sp"
        android:visibility="visible"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/botonWeb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-28dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="169dp"
        android:background="@drawable/bordes_redondos"
        android:clickable="true"
        android:elevation="1dp"
        android:fontFamily="@font/carme"
        android:text="@string/web"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/botonTiendas"
        android:layout_width="209dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-27dp"
        android:layout_marginTop="298dp"
        android:layout_marginEnd="229dp"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="@font/carme"
        android:text="@string/tiendas" />

    <Button
        android:id="@+id/botonConsumo"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-27dp"
        android:layout_marginTop="383dp"
        android:layout_marginEnd="198dp"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="@font/carme"
        android:text="@string/consumo" />

    <Button
        android:id="@+id/botonUsuario"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Titulo"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-26dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="256dp"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="@font/carme"
        android:text="@string/miPerfil"
        android:visibility="visible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="end|bottom"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#CFCFCF"
        android:src="@drawable/email"
        tools:ignore="SpeakableTextPresentCheck"></com.google.android.material.floatingactionbutton.FloatingActionButton>

</RelativeLayout>